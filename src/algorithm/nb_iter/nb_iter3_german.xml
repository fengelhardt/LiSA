<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE algorithm PUBLIC "" "LiSA.dtd">
<algorithm xmlns:LiSA="http://lisa.math.uni-magdeburg.de" name="Nachbarschaftssuche" type="iterative" call="nb_iter" code="external" help_file="algorithm/nb_iter.html">
  <heuristic>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Cmax"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Lmax"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Sum_Ci"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Sum_wiCi"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Sum_Ui"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Sum_wiUi"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Sum_Ti"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta/>
      <gamma objective="Sum_wiTi"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta release_times="yes"/>
      <gamma objective="Cmax"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta release_times="yes"/>
      <gamma objective="Lmax"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_Ci"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_wiCi"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_Ui"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_wiUi"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_Ti"/>
    </problem>
    <problem>
      <alpha env="O"/>
      <beta release_times="yes"/>
      <gamma objective="Sum_wiTi"/>
    </problem>
  </heuristic>
  <alg_controls>
    <integer name="STEPS" caption="Anzahl der Loesungen" default="5000"/>
    <integer name="NUMB_STUCKS" caption="Abbruch nach Stillstaenden (Abbruchkriterium)" default="10000"/>
    <integer name="PROB" caption="Anfangswahrscheinlichkeit" default="12"/>
    <integer name="MAX_STUCK" caption="Maximale Stillstandsanzahl" default="30"/>
    <real name="ABORT_BOUND" caption="Abbruchschranke (Abbruchkriterium)" default="0"/>
    <integer name="k" caption="k für k_API oder k_REINSERTION" default="1"/>
    <choice name="NGBH" caption="Nachbarschaft">
      <item name="CR_TST"/>
      <item name="k_API"/>
      <item name="SHIFT"/>
      <item name="PI"/>
      <item name="CR_API"/>
      <item name="BL_API"/>
      <item name="CR_SHIFT"/>
      <item name="BL_SHIFT"/>
      <item name="3_CR"/>
      <item name="k_REINSERTION"/>
    </choice>
    <choice name="METHOD" caption="Suchmethode">
      <item name="ThresholdAccepting"/>
    </choice>
    <fixed name="TYPE" value="RAND"/>
  </alg_controls>
</algorithm>
